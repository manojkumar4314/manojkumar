#include<stdio.h>
void main()
{
int c[10];
 int i,j,a;
printf("Enter the array elememt");
for(i=0;i<4;i++)
{
scanf("\n%d",&c[i]);
}
printf("\nSorted array");
for(i=0;i<4;i++)
{
for(j=i+1;j<4;j++)
{
if(c[i]>c[j])
{
a=c[i];
c[i]=c[j];
c[j]=a;
}
}
}
for(i=0;i<4;i++)
{
printf("\n sorted array element is %d",c[j]);
}
}
